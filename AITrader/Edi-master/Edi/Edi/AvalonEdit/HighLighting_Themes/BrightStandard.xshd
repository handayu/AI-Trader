<?xml version="1.0"?>
<!--
Edi enables the styling of syntax highlighting definitions
via color definitions that can be applied on named colors.
This is known as themeable highlighting in Edi.

Edi's implementation is based on Notepad++'s Settings>Style Configurator mechanism.
Review Notepad++'s lexer styles as this might be a good and simple source for more
highlighting themes in Edi.

WPF Color Scheme Overview
http://wpfknowledge.blogspot.de/2012/05/note-this-is-not-original-work.html
-->
<HlThemes name="Bright Standard" xmlns="http://edi.codeplex.com/highlightingtheme/syntaxdefinition/2013">
  <LexerStyles>
    <!-- HOWTO convert a theme from Notepad++ to Edi:

     Remove styleID, fontSize and fontName attributes
     convert fontStyle="0" into fontStyle="normal"
     convert fontStyle="1" into fontStyle="normal" fontWeight="bold"
     convert fontStyle="2" into fontStyle="italic"
     convert color values to .Net conform colors by adding '#FF' in background and foreground colors
    -->

    <LexerType name="ActionScript3">
      <WordsStyle name="Digits"  fgColor="#FF4B0082" fontStyle="normal" fontWeight="normal"/>
      <WordsStyle name="Comment" fgColor="#FFa31515" />

      <WordsStyle name="String"     fgColor="#FFa31515" />
      <WordsStyle name="MethodCall" fgColor="#FF00008D" fontWeight="normal" />

      <WordsStyle name="Keyword"  fgColor="#FF00004D" fontWeight="normal" />
      <WordsStyle name="Keyword1" fgColor="#FF00006D" fontWeight="normal" />

      <WordsStyle name="CommentMarkerSet"           fgColor="#FFFF8080" fontWeight="bold" />
      <WordsStyle name="CommentMarkerSetHackUndone" fgColor="#FFFF6060" fontWeight="bold" />
    </LexerType>

    <LexerType name="ASP/XHTML" desc="Color definitions for ASP (Active Server Page) highlighting pattern">
      <WordsStyle name="ASPSectionStartEndTags" fgColor="Black" bgColor="Yellow" /> 
      <WordsStyle name="ASPSection"             fgColor="Black" bgColor="#FFF7F2E3" />
    </LexerType>

